/* pages/individual/edit.wxss */
.content{
  border-color: rgb(192, 192, 190);
  border-style: solid;
  display: flex;
  flex-flow: row;
  border-width: 0px 0px 4px 0px;
  margin: -2px -5px 0px -5px;
  padding: 10px 10px 10px 10px;
  
}
.content p{
  font-size: 35rpx;
  font-family: 'Courier New', Courier, monospace;
}
.input{
  width: 70%;
  margin: 5px 5px 5px 5px;
  

}
.name_input{
  border-color: rgb(192, 192, 190);
  border-style: solid;  
  border-width: 1px;
  font-size: 10px;
  width: 100px;
  
  height: 12px;
}
.content_input{
  border-color: rgb(192, 192, 190);
  border-style: solid;  
  border-width: 1px;
  height: 100px;
  font-size: 10px;
  
  
  width: 100px;
}
.circle{
  border-radius: 50%;
  max-width: 70px;
  height: 70px;
  background-color: blue;
  font-size:5px;
  font-family: 'Courier New', Courier, monospace;
  color: white;
  margin: 5px 5px 5px 5px;
  float:right;  
  text-align: center;
}
.circle p{
  font-size: 25rpx;
  font-family: 'Courier New', Courier, monospace;
}
.delete{
  border-radius: 5px;
  background-color: green;
  max-width: 100px;
  height: 30px;
  float: right;
  font-size: 10px;
  font-family: 'Courier New', Courier, monospace;
  color: white;
 
}
.btn{
  border-radius: 5px;
  background-color: rgb(139, 170, 236);
  font-size: 15px;
  font-family: 'Courier New', Courier, monospace;
  color: white;
  margin: 10px 10px 10px 10px;
}
