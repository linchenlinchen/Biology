<!--pages/individual/query_result.wxml-->
<view>
  <view>
    <input type='text' placeholder='按照手机号搜索志愿者' class="search"></input>
  </view>
  <view wx:for="{{result}}" wx:for-item="item" class="content">
     <p>{{username}}: {{item.username}}</p>
     <view class="line"></view>
     <p>{{recognition}}: {{item.recognition}}</p>
     <view class="line"></view>
     <p>{{shape}}: {{item.shape}}</p>
     <view class="line"></view>
     <p>{{record}}: {{item.record}}</p>
     <view class="line"></view>
     <p>{{brain}}: {{item.brain}}</p>
     <view class="line"></view>
     <p>{{other}}: {{item.other}}</p>
  </view>
  <view>
    <button class="button" bindtap="backManage"><p>返回问卷管理页</p></button>
  </view>
</view>
